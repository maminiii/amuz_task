List = commentRes.data;\n\n      commentList.forEach((comment) => {\n        store.dispatch(\n          createComment({\n            id: comment.id,\n            postId: comment.postId,\n            body: comment.body,\n            email: comment.email,\n            name: comment.name,\n          })\n        );\n      });\n    }\n    console.log(\"##fetchComment end\");\n  } catch (e) {\n    console.log(\"##error\", e);\n  }\n};\n\nconst init = () => {\n  fetchUser();\n  fetchPost();\n  fetchComment();\n};\n\ninit();\nexport default store;\n",[],"/Users/developer/task/task3/src/store/modelDucks/User.js",[],[],"/Users/developer/task/task3/src/store/modelDucks/Post.js",[],[],"/Users/developer/task/task3/src/store/orm/index.js",[],[],"/Users/developer/task/task3/src/store/modelDucks/models.js",[],[],"/Users/developer/task/task3/src/components/Posts/Container.js",[],[],"/Users/developer/task/task3/src/components/Posts/UserDropdown.js",[],[],"/Users/developer/task/task3/src/components/Posts/PostInput.js",["347"],[],"/Users/developer/task/task3/src/store/modelDucks/PostSelectors.js",[],[],"/Users/developer/task/task3/src/store/modelDucks/UserSelectors.js",[],[],"/Users/developer/task/task3/src/components/Posts/PostList.js",[],[],"/Users/developer/task/task3/src/components/Posts/PostItem.js",[],[],"/Users/developer/task/task3/src/store/modelDucks/Comment.js",[],[],"/Users/developer/task/task3/src/store/ducks/currentPost.js",[],[],"/Users/developer/task/task3/src/store/modelDucks/CommentSelectors.js",[],[],"/Users/developer/task/task3/src/components/CommentContainer.js",["348","349","350","351","352","353","354","355"],[],{"ruleId":"356","severity":1,"message":"357","line":1,"column":28,"nodeType":"358","messageId":"359","endLine":1,"endColumn":43},{"ruleId":"356","severity":1,"message":"360","line":1,"column":45,"nodeType":"358","messageId":"359","endLine":1,"endColumn":53},{"ruleId":"356","severity":1,"message":"361","line":3,"column":22,"nodeType":"358","messageId":"359"